<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <add key="globalPackagesFolder" value="artifacts/nuget-cache" />
  </config>

  <packageSources>
    <clear />
    <add key="local" value="artifacts/nuget" />
    <add key="github" value="https://nuget.pkg.github.com/pxbunny/index.json" />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
  </packageSources>

  <packageSourceMapping>
    <packageSource key="local">
      <package pattern="Integrations.*" />
    </packageSource>
    <packageSource key="github">
      <package pattern="Integrations.*" />
    </packageSource>
    <packageSource key="nuget">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
