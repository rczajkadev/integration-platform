{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Default": "Information",
      "Function": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Information",
      "Microsoft": "Warning",
      "Worker": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}
